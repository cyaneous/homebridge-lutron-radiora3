    "devDependencies": {
        "@types/jest": "^29.2.2",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "eslint": "^8.27.0",
        "homebridge": "^1.2.3",
        "homebridge-config-ui-x": "^4.42.0",
        "jest": "^29.3.1",
        "jest-mock": "^29.3.1",
        "nodemon": "^2.0.6",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.0.5"
    }
